                    if (Convert.ToString(MM).Length == 1)
                    {
                        SelectitemsHR.Add(new SelectListItem { Text = ("0" + Convert.ToString(MM)), Value = ("0" + Convert.ToString(MM)) });
                    }
                    else
                    {
                        SelectitemsHR.Add(new SelectListItem { Text = Convert.ToString(MM), Value = Convert.ToString(MM) });
                    }
                }

                List<SelectListItem> SelectitemsMin = new List<SelectListItem>();
                for (int MM = 0; MM <= 59; MM++)
                {
                    if (Convert.ToString(MM).Length == 1)
                    {
                        SelectitemsMin.Add(new SelectListItem { Text = ("0" + Convert.ToString(MM)), Value = ("0" + Convert.ToString(MM)) });
                    }
                    else
                    {
                        SelectitemsMin.Add(new SelectListItem { Text = Convert.ToString(MM), Value = Convert.ToString(MM) });
                    }
