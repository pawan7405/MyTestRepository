        $(document).ready(function () {

            $("#ddlQueueID").change(function () {

                url = $('#AppName').val() + '/WAT/FetchSubQueue?ParentID=' + this.value;
                $.get(url, function (data) {
                    $('#ddlSubQueueID').empty();
                    var jsonstring = JSON.parse(data)

                    $('#ddlSubQueueID').append($("<option>").val("").html("- Select -"));
                    $(jsonstring).each(function (index) {
                        $('#ddlSubQueueID').append($("<option>").val(jsonstring[index].ID).html(jsonstring[index].Name));
                    });

                });
            });
            var ActionTime = parseInt($("#ActionTime").val());
        });
        function HideShowTOP(e) {

            if (e == 'alertMessage')
                $("#" + e).slideToggle();
            else
                $("#" + e.id).slideToggle();
        }
