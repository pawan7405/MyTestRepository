                            </li>
                            <li>
                                <input type="submit" value="TL/AM Meeting" onclick="ChangeStatus(27,this);" />
                            </li>
                            <li>
                                <input type="submit" value="Cab Downtime" onclick="ChangeStatus(30,this);" />
                            </li>
                            <li>
                                <input type="submit" value="Sick Downtime" onclick="ChangeStatus(29,this);" />
                            </li>
                            <li>
                                <input type="submit" value="Quality Feedback" onclick="ChangeStatus(28,this);" />
                            </li>
                            <li>
                                <input type="submit" value="No Work" onclick="ChangeStatus(34,this);" />
                            </li>
                            <li>
                                <input type="submit" value="IT Downtime" onclick="ChangeStatus(31,this);" />
                            </li>*@
                    </ul>
